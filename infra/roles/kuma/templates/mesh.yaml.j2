---
type: Mesh
name: {{ env }}
mtls:
  enabledBackend: {{ env }}-ca
  backends:
    - name: {{ env }}-ca
      type: builtin
  enabled: true
logging:
  defaultBackend: {{ env }}-file
  backends:
    - name: {{ env }}-file
      type: file
      conf:
        path: /var/log/kuma/{{ env }}-traffic-all.log
