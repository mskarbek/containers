[epel]
name=Extra Packages for Enterprise Linux 8 - $basearch
baseurl=https://ftp.fi.muni.cz/pub/linux/fedora/epel/8/Everything/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-8

[epel-modular]
name=Extra Packages for Enterprise Linux Modular 8 - $basearch
baseurl=https://ftp.fi.muni.cz/pub/linux/fedora/epel/8/Modular/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-8
